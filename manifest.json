{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "permissions": [
          "webRequest",
          "*://*.google.com/"
  ],
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "bower_components/jquery-2.1.4.min/jquery-2.1.4.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html"
}